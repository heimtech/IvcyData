{
    "name":"terminal42/contao-pageimage",
    "description":"pageimage extension for Contao Open Source CMS",
    "keywords":["contao", "page", "image", "background"],
    "type":"contao-module",
    "license":"LGPL-3.0+",
    "authors":[
        {
            "name":"terminal42 gmbh",
            "homepage":"http://www.terminal42.ch"
        }
    ],
    "require":{
        "php":">=5.3.2",
        "contao/core":"^3.2.0",
        "contao-community-alliance/composer-plugin":"2.*"
    },
    "replace": {
        "contao-legacy/pageimage": "self.version"
    },
    "extra":{
        "contao": {
            "sources":{
                "": "system/modules/pageimage"
            }
        }
    }
}
